{
  "name": "frc-devtools",
  "displayName": "FRC Development Tools",
  "description": "This is the help text",
  "version": "0.1.0-DEV",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "frc-devtools.openCtreDocs",
        "title": "FRC Dev Tools: Open CTRE documentation"
      }
    ]
